{
    "manifest_version":2,
    "version":"1.0",
    "name":"Lyrics Button",
    "content_scripts":[
        {
         "matches":["*://www.youtube.com/*"],
         "js":["content_script.js"]
        }
      ],
    "background":{
        "scripts":["background.js"]
    },
      "permissions": [
        "tabs",
        "<all_urls>",
        "activeTab"
    ],
    "icons": {
      "96": "./icons/icon-96.png",
      "48": "./icons/icon-48.png"
    },
    "browser_action":{
      "default_icon":{
        "38":"icon-38.png",
        "19":"icon-19.png"
      }
     }
        
  }
  