{
    "manifest_version":2,
    "version":"1.0",
    "name":"Lyrics Button",
    "content_scripts":[
        {
         "matches":["*://www.youtube.com/*"],
         "js":["content_script.js"]
        }
      ],
    "background":{
        "scripts":["background.js"]
    },
      "permissions": [
        "tabs",
        "<all_urls>",
        "activeTab",
        "storage"
    ],
    "icons": {
      "48": "./icons/icon-48.png",
      "96": "./icons/icon-96.png"
    },
    "browser_action":{
      "default_icon":{
        "19":"./icons/icon-19.png",
        "38":"./icons/icon-38.png"
      },
      "default_title": "Lyrics Button",
      "default_popup": "popup.html"
     }
        
  }
  